<?xml version="1.0" encoding="UTF-8" ?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
  <fullName>Daily_end_date_valid</fullName>
  <active>true</active>
  <errorConditionFormula>Start__c &gt;= DATETIMEVALUE(DailyEnd__c)</errorConditionFormula>
  <errorDisplayField>Start__c</errorDisplayField>
  <errorMessage>The start date &amp; time must be before the final end date.</errorMessage>
</ValidationRule>
